{
  "name": "jonny-cho.github.io",
  "private": false,
  "description": "A neat blog theme for developers",
  "version": "3.0.0",
  "author": "jonny-cho <andante2183@gmail.com>",
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "gatsby": "^5.14.5",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sass": "^6.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-remark-autolink-headers": "^6.14.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-remark-smartypants": "^6.14.0",
    "gatsby-remark-table-of-contents": "^2.0.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-rotating-text": "^1.4.1",
    "sass": "^1.89.2",
    "typeface-montserrat": "^1.1.13"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "core-js": "^3.39.0",
    "path-to-regexp": "^0.1.12",
    "cookie": "^0.7.0",
    "postcss": "^8.4.31",
    "lodash.template": "^4.5.0",
    "workbox-build": "^6.6.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-theme"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "build:ci": "gatsby clean && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "deploy": "npm run build",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "new:post": "node scripts/new-post.js",
    "new:algorithm": "node scripts/new-post.js --type=algorithm",
    "new:spring": "node scripts/new-post.js --type=spring",
    "new:git": "node scripts/new-post.js --type=git",
    "new:java": "node scripts/new-post.js --type=java",
    "validate:posts": "node scripts/validate-post-structure.js",
    "validate:file": "node scripts/validate-post-structure.js",
    "standardize:existing": "node scripts/standardize-posts.js",
    "standardize:preview": "node scripts/standardize-posts.js --dry-run",
    "standardize:backup": "node scripts/standardize-posts.js --backup",
    "analyze": "npx webpack-bundle-analyzer public/webpack.stats.json",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer public/webpack.stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonny-cho"
  },
  "bugs": {
    "url": "https://github.com/Jonny-Cho/jonny-cho.github.io/issues"
  }
}
